{
  String lang=getLanguageFromMetadata(page.getData().getParseMeta());
  if (lang == null) {
    LanguageParser parser=new LanguageParser(doc);
    lang=parser.getLanguage();
  }
  if (lang != null) {
    return lang;
  }
  lang=page.getData().getContentMeta().get(Response.CONTENT_LANGUAGE);
  return lang;
}
