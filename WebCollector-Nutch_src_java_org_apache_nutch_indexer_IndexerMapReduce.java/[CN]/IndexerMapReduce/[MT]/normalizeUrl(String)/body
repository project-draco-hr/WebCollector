{
  if (!normalize) {
    return url;
  }
  String normalized=null;
  if (urlNormalizers != null) {
    try {
      normalized=urlNormalizers.normalize(url,URLNormalizers.SCOPE_INDEXER);
      normalized=normalized.trim();
    }
 catch (    Exception e) {
      LOG.warn("Skipping " + url + ":"+ e);
      normalized=null;
    }
  }
  return normalized;
}
