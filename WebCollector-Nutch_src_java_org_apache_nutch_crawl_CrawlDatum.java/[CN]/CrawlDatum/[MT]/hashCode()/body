{
  int res=0;
  if (signature != null) {
    for (int i=0; i < signature.length / 4; i+=4) {
      res^=(signature[i] << 24 + signature[i + 1] << 16 + signature[i + 2] << 8 + signature[i + 3]);
    }
  }
  if (metaData != null) {
    res^=metaData.entrySet().hashCode();
  }
  return res ^ status ^ ((int)fetchTime)^ ((int)modifiedTime)^ retries^ fetchInterval^ Float.floatToIntBits(score);
}
