{
  config=conf;
  String path=conf.get("dummy.path");
  if (path == null) {
    String message="Missing path. Should be set via -Ddummy.path";
    message+="\n" + describe();
    LOG.error(message);
    throw new RuntimeException(message);
  }
  try {
    writer=new BufferedWriter(new FileWriter(conf.get("dummy.path")));
  }
 catch (  IOException e) {
  }
}
