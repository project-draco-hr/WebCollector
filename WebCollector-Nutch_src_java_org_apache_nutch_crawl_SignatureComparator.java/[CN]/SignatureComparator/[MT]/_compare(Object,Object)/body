{
  if (o1 == null && o2 == null)   return 0;
  if (o1 == null)   return -1;
  if (o2 == null)   return 1;
  if (!(o1 instanceof byte[]))   return -1;
  if (!(o2 instanceof byte[]))   return 1;
  byte[] data1=(byte[])o1;
  byte[] data2=(byte[])o2;
  return _compare(data1,0,data1.length,data2,0,data2.length);
}
