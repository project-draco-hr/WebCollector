{
  if (curURL != null) {
    if ("ExternalPage".equals(qName)) {
      System.out.println(curURL);
      pages++;
      if (title.length() > 0) {
        title.delete(0,title.length());
      }
      if (desc.length() > 0) {
        desc.delete(0,desc.length());
      }
      curURL=null;
    }
 else     if ("d:Title".equals(qName)) {
      titlePending=false;
    }
 else     if ("d:Description".equals(qName)) {
      descPending=false;
    }
  }
}
