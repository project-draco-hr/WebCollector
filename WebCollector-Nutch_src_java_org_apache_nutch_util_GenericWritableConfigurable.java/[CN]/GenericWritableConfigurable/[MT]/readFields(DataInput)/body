{
  byte type=in.readByte();
  Class<?> clazz=getTypes()[type];
  try {
    set((Writable)clazz.newInstance());
  }
 catch (  Exception e) {
    e.printStackTrace();
    throw new IOException("Cannot initialize the class: " + clazz);
  }
  Writable w=get();
  if (w instanceof Configurable)   ((Configurable)w).setConf(conf);
  w.readFields(in);
}
