{
  int count=0;
  for (  String id : queues.keySet()) {
    FetchItemQueue fiq=queues.get(id);
    if (fiq.getQueueSize() == 0)     continue;
    LOG.info("* queue: " + id + " >> dropping! ");
    int deleted=fiq.emptyQueue();
    for (int i=0; i < deleted; i++) {
      totalSize.decrementAndGet();
    }
    count+=deleted;
  }
  return count;
}
