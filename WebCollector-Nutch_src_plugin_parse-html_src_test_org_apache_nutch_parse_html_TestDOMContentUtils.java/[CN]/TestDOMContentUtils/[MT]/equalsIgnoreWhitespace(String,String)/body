{
  StringTokenizer st1=new StringTokenizer(s1);
  StringTokenizer st2=new StringTokenizer(s2);
  while (st1.hasMoreTokens()) {
    if (!st2.hasMoreTokens())     return false;
    if (!st1.nextToken().equals(st2.nextToken()))     return false;
  }
  if (st2.hasMoreTokens())   return false;
  return true;
}
