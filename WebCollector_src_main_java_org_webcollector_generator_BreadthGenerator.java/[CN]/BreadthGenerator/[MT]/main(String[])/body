{
  String crawl_path="/home/hu/data/crawl_test";
  BreadthGenerator bg=new BreadthGenerator(null){
    @Override public boolean shouldFilter(    Page page){
      if (Pattern.matches("http://news.xinhuanet.com/world/.*",page.url))       return false;
 else       return true;
    }
  }
;
  bg.topN=20;
  bg.run(crawl_path);
}
