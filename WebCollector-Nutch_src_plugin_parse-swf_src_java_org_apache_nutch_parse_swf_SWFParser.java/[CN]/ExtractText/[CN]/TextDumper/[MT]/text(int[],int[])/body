{
  int[] codes=(int[])fontCodes.get(fontId);
  if (codes == null) {
    strings.add("\n**** ?????????????? ****\n");
    return;
  }
  char[] chars=new char[glyphIndices.length];
  for (int i=0; i < chars.length; i++) {
    int index=glyphIndices[i];
    if (index >= codes.length) {
      chars[i]=(char)index;
    }
 else {
      chars[i]=(char)(codes[index]);
    }
  }
  strings.add(new String(chars));
}
