{
  if (handler == null) {
    String infostr="";
    for (    String info : infos) {
      infostr+=info + " ";
    }
switch (type) {
case Log.INFO:
      defaultLogger.info(infostr);
    break;
case Log.ERROR:
  defaultLogger.error(infostr);
break;
}
}
 else {
Message msg=new Message();
msg.what=type;
msg.obj=infos;
handler.sendMessage(msg);
}
}
