{
  BufferedReader in=new BufferedReader(new InputStreamReader(System.in));
  String line;
  while ((line=in.readLine()) != null) {
    String out=filter.filter(line);
    if (out != null) {
      System.out.print("+");
      System.out.println(out);
    }
 else {
      System.out.print("-");
      System.out.println(line);
    }
  }
}
