{
  if (!resumable) {
    if (seeds.size() == 0) {
      Log.Info("error","Please add at least one seed");
      return;
    }
    if (regexs.size() == 0) {
      autoRegex();
    }
  }
  if (!resumable) {
    inject();
  }
  initGenerator();
  for (int i=0; i < depth; i++) {
    Log.Info("info","starting depth " + (i + 1));
    if (generator != null)     generate();
 else     break;
  }
}
