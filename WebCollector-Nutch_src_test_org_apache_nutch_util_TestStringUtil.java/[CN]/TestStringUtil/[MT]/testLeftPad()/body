{
  String s="my string";
  String ps=StringUtil.leftPad(s,0);
  Assert.assertTrue(s.equals(ps));
  ps=StringUtil.leftPad(s,9);
  Assert.assertTrue(s.equals(ps));
  ps=StringUtil.leftPad(s,10);
  Assert.assertTrue((" " + s).equals(ps));
  ps=StringUtil.leftPad(s,15);
  Assert.assertTrue(("      " + s).equals(ps));
}
