{
  try {
    DbUpdater dbUpdater=createDbUpdater();
    if (dbUpdater != null) {
      fetcher.setDbUpdater(dbUpdater);
    }
    fetcher.setRequestFactory(this);
    fetcher.setParserFactory(this);
    fetcher.setHandler(createFetcherHandler());
    return fetcher;
  }
 catch (  Exception ex) {
    LogUtils.getLogger().info("Exception",ex);
    return null;
  }
}
