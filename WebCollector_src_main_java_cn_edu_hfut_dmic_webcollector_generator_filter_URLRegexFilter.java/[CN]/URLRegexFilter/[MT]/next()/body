{
  while (true) {
    CrawlDatum crawldatum=generator.next();
    if (crawldatum == null) {
      return null;
    }
    String url=crawldatum.getUrl();
    int state=0;
    for (    String nregex : negative) {
      if (Pattern.matches(nregex,url)) {
        state=1;
        break;
      }
    }
    if (state == 1) {
      continue;
    }
    int count=0;
    for (    String pregex : positive) {
      if (Pattern.matches(pregex,url)) {
        count++;
      }
    }
    if (count == 0) {
      continue;
    }
 else {
      return crawldatum;
    }
  }
}
