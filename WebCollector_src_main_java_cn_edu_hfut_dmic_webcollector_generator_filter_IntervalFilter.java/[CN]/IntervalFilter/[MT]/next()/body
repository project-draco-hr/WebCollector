{
  while (true) {
    CrawlDatum crawldatum=generator.next();
    if (crawldatum == null) {
      return null;
    }
    if (crawldatum.getStatus() == CrawlDatum.STATUS_DB_UNFETCHED) {
      return crawldatum;
    }
    if (Config.interval == -1) {
      continue;
    }
    Long lasttime=crawldatum.getFetchTime();
    if (lasttime + Config.interval > System.currentTimeMillis()) {
      continue;
    }
    return crawldatum;
  }
}
