{
  boolean hasMore=true;
  while (hasMore) {
    int feed=size - queue.getSize();
    if (feed <= 0) {
      try {
        Thread.sleep(1000);
      }
 catch (      InterruptedException ex) {
      }
      continue;
    }
    while (feed > 0 && hasMore) {
      CrawlDatum datum=generator.next();
      hasMore=(datum != null);
      if (hasMore) {
        queue.addFetchItem(new FetchItem(datum));
        feed--;
      }
    }
  }
}
