{
  for (  SegmentMergeFilter filter : filters) {
    if (!filter.filter(key,generateData,fetchData,sigData,content,parseData,parseText,linked)) {
      if (LOG.isTraceEnabled())       LOG.trace("Key " + key + " dropped by "+ filter.getClass().getName());
      return false;
    }
  }
  if (LOG.isTraceEnabled())   LOG.trace("Key " + key + " accepted for merge.");
  return true;
}
