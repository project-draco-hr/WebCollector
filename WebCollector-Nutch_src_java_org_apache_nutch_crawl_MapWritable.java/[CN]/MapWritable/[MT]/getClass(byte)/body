{
  Class<?> clazz=ID_CLASS_MAP.get(new Byte(id));
  if (clazz == null) {
    ClassIdEntry entry=fIdFirst;
    while (entry != null) {
      if (entry.fId == id) {
        return entry.fclazz;
      }
      entry=entry.fNextIdEntry;
    }
  }
 else {
    return clazz;
  }
  throw new IOException("unable to load class for id: " + id);
}
