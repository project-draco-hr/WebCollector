{
  if (path == null) {
    return false;
  }
  if (!PATH_PATTERN.matcher(path).matches()) {
    return false;
  }
  int slash2Count=countToken("//",path);
  int slashCount=countToken("/",path);
  int dot2Count=countToken("..",path);
  return (dot2Count <= 0) || ((slashCount - slash2Count - 1) > dot2Count);
}
