{
  String info_path=Config.current_info_path;
  File inject_file=new File(crawl_path,info_path);
  if (!inject_file.getParentFile().exists()) {
    inject_file.getParentFile().mkdirs();
  }
  DbWriter<CrawlDatum> writer;
  if (inject_file.exists())   writer=new DbWriter<CrawlDatum>(CrawlDatum.class,inject_file,append);
 else   writer=new DbWriter<CrawlDatum>(CrawlDatum.class,inject_file,false);
  for (  String url : urls) {
    CrawlDatum crawldatum=new CrawlDatum();
    crawldatum.url=url;
    crawldatum.status=Page.UNFETCHED;
    writer.write(crawldatum);
  }
  writer.close();
}
